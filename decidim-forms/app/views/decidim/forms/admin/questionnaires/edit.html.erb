<%= decidim_form_for(@form, url: url_for(questionnaire.questionnaire_for), method: :post, html: { class: "form edit_questionnaire" }) do |form| %>
  <%= render partial: "decidim/forms/admin/questionnaires/form", object: form, locals: { title: t(".title") } %>

  <div class="button--double form-general-submit">
    <%= form.submit t(".save"), class: "button" %>
  </div>
<% end %>
