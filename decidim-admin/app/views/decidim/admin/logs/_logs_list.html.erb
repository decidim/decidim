<div class="card">
  <div class="card-divider">
    <h2 class="card-title">
      <%= t "decidim.admin.titles.admin_log" %>
    </h2>
  </div>
  <div class="table-scroll">
    <% if logs.any? %>
      <ul class="logs table">
        <% logs.each do |log| %>
          <%= render_log(log) %>
        <% end %>
      </ul>
    <% else %>
      <div class="logs table">
        <p class="logs--no-logs"><%= t(".no_logs_yet") %></p>
      </div>
    <% end %>
  </div>
</div>
