<div class="card">
  <div class="card-divider">
    <h2 class="card-title">
      <%= t "decidim.admin.titles.admin_log" %>
    </h2>
  </div>
  <div class="table-scroll">
    <ul class="logs table">
      <% logs.each do |log| %>
        <%= render_log(log) %>
      <% end %>
    </ul>
  </div>
</div>
