{
  "env": {
    "browser": true,
    "es6": false
  },
  "extends": "airbnb",
  "globals": {
    "$": false,
    "jQuery": false,
    "ReactDOM": false,
    "React": false,
    "GraphQLDocs": false,
    "Quill": false
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "comma-dangle": ["error", "never"]
  }
}
