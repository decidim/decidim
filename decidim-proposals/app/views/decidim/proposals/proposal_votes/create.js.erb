var $proposalVoteButton = $('#proposal-<%= @proposal.id %>-vote-button');

$proposalVotesCount.html('<%= j(render partial: 'decidim/proposals/proposals/votes_count', locals: { proposal: @proposal, number_class: @number_class, label_class: @label_class }) %>');
