{
  "presets": [
    [
      "@babel/preset-env", {
        "forceAllTransforms": true,
        "useBuiltIns": "entry",
        "corejs": 3,
        "modules": false
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    "@babel/plugin-transform-classes",
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true,
        "corejs": false
      }
    ],
    ["@babel/plugin-transform-regenerator", { "async": false }]
  ]
}
